def_key "+"
    show_clock
def_key "="
    volume_up

def_key "h"
  seek_backward
def_key "l"
  seek_forward

def_key "h"
    previous_column
def_key "l"
    next_column

def_key "j"
    scroll_down
def_key "k"
    scroll_up

def_key "J"
    move_sort_order_down
def_key "K"
    move_sort_order_up

def_key "J"
    move_selected_items_down
def_key "K"
    move_selected_items_up

def_key "ctrl-u"
    page_up
def_key "ctrl-d"
    page_down

def_key "d"
  delete_playlist_items

def_key "."
    show_lyrics

def_key "n"
    next_found_item
def_key "N"
    previous_found_item

